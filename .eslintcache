ssfully\r\n                                console.log('File Deleted')\r\n                                let newArray = imgUrl.filter(\r\n                                  (image) => image !== img\r\n                                )\r\n\r\n                                setImgUrl(newArray)\r\n                              })\r\n                              .catch((error) => {\r\n                                console.log(error)\r\n                              })\r\n                          }}\r\n                          color=\"secondary\"\r\n                          badgeContent={\r\n                            <Button style={{ color: 'white' }}>Delete</Button>\r\n                          }\r\n                        >\r\n                          <img\r\n                            style={{\r\n                              maxHeight: '80px',\r\n                              width: 'auto',\r\n                            }}\r\n                            src={img}\r\n                          />\r\n                        </Badge>\r\n                      </div>\r\n                    ))}\r\n                  </div>\r\n                </>\r\n              )}\r\n            </div>\r\n          </Paper>\r\n\r\n          {loading && (\r\n            <div style={{ textAlign: 'center', margin: '15px 0' }}>\r\n              <CircularProgress color=\"black\" />\r\n            </div>\r\n          )}\r\n          {error && (\r\n            <Alert\r\n              style={{ margin: '15px 0' }}\r\n              severity=\"error\"\r\n              onClose={() => {\r\n                setState({ ...state, error: false })\r\n              }}\r\n            >\r\n              {error}\r\n            </Alert>\r\n          )}\r\n          {success && (\r\n            <Alert\r\n              style={{ margin: '15px 0' }}\r\n              severity=\"success\"\r\n              onClose={() => {\r\n                setState({ ...state, success: false })\r\n              }}\r\n      