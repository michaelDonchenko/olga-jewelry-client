                 <HomeIcon className={classes.navIcon}></HomeIcon>\r\n                  </Button>\r\n                </NavLink>\r\n              </Tooltip>\r\n\r\n              <Tooltip placement=\"bottom-start\" title=\"Shop\">\r\n                <NavLink\r\n                  activeStyle={{\r\n                    color: '#0277bd',\r\n                    backgroundColor: '#e1f5fe',\r\n                    padding: '10px 0 12px 0',\r\n                  }}\r\n                  to=\"/shop\"\r\n                  className={classes.link}\r\n                >\r\n                  <Button color=\"inherit\">\r\n                    <ShopIcon className={classes.navIcon}></ShopIcon>\r\n                  </Button>\r\n                </NavLink>\r\n              </Tooltip>\r\n\r\n              <Tooltip placement=\"bottom-start\" title=\"Cart\">\r\n                <NavLink\r\n                  activeStyle={{\r\n                    color: '#0277bd',\r\n                    backgroundColor: '#e1f5fe',\r\n                    padding: '10px 0 12px 0',\r\n                  }}\r\n                  to=\"/cart\"\r\n                  className={classes.link}\r\n                >\r\n                  <StyledBadge\r\n                    badgeContent={cart && cart.length}\r\n                    color=\"secondary\"\r\n                  >\r\n                    <Button color=\"inherit\">\r\n                      <ShoppingCartIcon\r\n                        className={classes.navIcon}\r\n                      ></ShoppingCartIcon>\r\n                    </Button>\r\n                  </StyledBadge>\r\n                </NavLink>\r\n              </Tooltip>\r\n            </Hidden>\r\n          </Typography>\r\n\r\n          {!user && (\r\n            <>\r\n              <NavLink\r\n                activeStyle={{\r\n                  color: '#0277bd',\r\n                  backgroundColor: '#e1f5fe',\r\n                }}\r\n                to=\"/login\"\r\n                className={classes.link}\r\n              >\r\n                <Button color=\"inherit\">\r\n                  <span>Login</span>\r\n                </Button>\r\n              </NavLink>\r\n\r\n              <NavLink\r\n                activeStyle={{\r\n                  color: '#0277bd',\r\n                  backgroundColor: '#e1f5fe',\r\n                }}\r\n                to=\"/register\"\r\n                className={classes.link}\r\n              >\r\n                <Button color=\"inherit\">\r\n                  <span>Register</span>\r\n                </Button>\r\n              </NavLink>\r\n            </>\r\n          )}\r\n          {user && user.role === 'subscriber' && (\r\n            <Tooltip\r\n              placement=\"bottom-start\"\r\n              title={`menu for user ${user.email}`}\r\n            >\r\n              <Button\r\n                aria-controls=\"simple-menu\"\r\n                aria-haspopup=\"true\"\r\n                onClick={handleClick}\r\n                startIcon={<AccountCircleIcon style={{ fontSize: '1.5rem' }} />}\r\n                className={classes.link}\r\n                color=\"inherit\"\r\n              >\r\n                <span>User Menu</span>\r\n              </Button>\r\n            </Tooltip>\r\n          )}\r\n\r\n          {user && user.role === 'admin' && (\r\n            <NavLink\r\n              style={{ padding: '5px' }}\r\n              activeStyle={{\r\n                color: '#0277bd',\r\n                backgroundColor: '#e1f5fe',\r\n              }}\r\n              to=\"/admin/dashboard\"\r\n              className={classes.link}\r\n            >\r\n              <Button\r\n                startIcon={<DashboardIcon />}\r\n                className={classes.link}\r\n                color=\"inherit\"\r\n              >\r\n                <span>Admin Dashboard</span>\r\n              </Button>\r\n            </NavLink>\r\n          )}\r\n        </Toolbar>\r\n\r\n        <Menu\r\n          id=\"simple-menu\"\r\n          ancho